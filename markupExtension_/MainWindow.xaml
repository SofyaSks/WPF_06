<Window x:Class="markupExtension_.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:markupExtension_"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" FontSize="48">
    <StackPanel>
        <TextBlock>your new random password</TextBlock>
        <TextBox Text="{local:randomPassword Length=7}"/>
    </StackPanel>
</Window>
